{
    "compilerOptions": {
      "target": "ES2022",
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "outDir": "../build/api",
      "forceConsistentCasingInFileNames": true,
      "module": "CommonJS",
      "resolveJsonModule": true,
      "isolatedModules": false
    },
  }
  